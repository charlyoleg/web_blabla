{
  "compilerOptions": {
    "target": "es6",
    "module": "es6",
    "strict": false,
    "strictNullChecks": false,
    "strictPropertyInitialization": false,
    "noImplicitAny": true,
    "sourceMap": true,
    "typeRoots": ["node_modules/@types"],
    "removeComments": false,
    "outDir": "../dist/js"
  },
  "include": [
    "**/*"
  ],
  "exclude": [
    "**/*.test.ts"
  ]
}
